<div class="container">
<% if current_user %>
  Welcome, <%= current_user.email%>!
  <button class="btn btn-default btn-xs"><%= link_to 'Logout', log_out_path(@user) %></button>
  <button class="btn btn-primary btn-xs">Add photo</button>
  <% else %>
  <button class="btn btn-default btn-xs"><%= link_to 'Login', log_in_path(@user) %></button>
  <button class="btn btn-default btn-xs"><%= link_to 'Create Account', new_user_path(@user) %></button>
  <% end %>

<h1>Photo Sharing!<small> Under construction</small></h1>

</div>
