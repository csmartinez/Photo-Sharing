<h1>Sign Up</h1>
<%=form_for User.new do |f| %>
  <%=f.label :email %>
  <%=f.text_field :email %><br>
  <%=f.label :password %>
  <%=f.password_field :password %><br>
  <%=f.label :password_confirmation %>
  <%=f.password_field :password_confirmation %><br>
  <%= f.file_field :avatar %>
  <button class="btn btn-primary btn-xs">Sign Up</button>
<% end %>
